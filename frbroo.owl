    <!-- http://erlangen-crm.org/efrbroo/R1_is_logical_successor_of -->

    <owl:ObjectProperty rdf:about="http://erlangen-crm.org/efrbroo/R1_is_logical_successor_of">
        <rdfs:subPropertyOf rdf:resource="http://erlangen-crm.org/current/P130_shows_features_of"/>
