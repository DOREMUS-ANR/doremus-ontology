         <owl:Restriction>
            <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:cardinality>
            <owl:onProperty>
               <owl:ObjectProperty rdf:about="http://erlangen-crm.org/efrbroo/R63_named"/>
            </owl:onProperty>
         </owl:Restriction>
      </rdfs:subClassOf>
      <rdfs:subClassOf>
