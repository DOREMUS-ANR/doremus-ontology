   <!--http://erlangen-crm.org/efrbroo/R39_is_intended_for-->
   <owl:ObjectProperty rdf:about="http://erlangen-crm.org/efrbroo/R39_is_intended_for">
      <rdfs:label xml:lang="en">R39 is intended for</rdfs:label>
      <owl:inverseOf rdf:resource="http://erlangen-crm.org/efrbroo/R39i_is_target_audience_in"/>
