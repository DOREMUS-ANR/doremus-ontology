   <!--http://erlangen-crm.org/efrbroo/R63_named-->
   <owl:ObjectProperty rdf:about="http://erlangen-crm.org/efrbroo/R63_named">
      <rdfs:label xml:lang="en">R63 named</rdfs:label>
      <owl:inverseOf rdf:resource="http://erlangen-crm.org/efrbroo/R63i_was_named_by"/>
      <rdfs:domain rdf:resource="http://erlangen-crm.org/efrbroo/F52_Name_Use_Activity"/>
